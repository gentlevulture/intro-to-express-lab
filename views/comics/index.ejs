<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Unread Comix</title>
</head>
<body>
  <h1>Comics</h1>
  <% if(error) { %>
    <h2><%= error %></h2>
  <% } %>
  <ul>
    <% comics.forEach(function(comic) { %>
      <li>
        <%= comic.text %>
          - 
        <%= comic.done ? 'done' : 'not done' %>
      </li>
    <% }) %>
  </ul>
</body>
</html>